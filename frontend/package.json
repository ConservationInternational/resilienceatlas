  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "cypress run --headless --browser chrome",
    "test:open": "cypress open",
    "lint": "eslint ./src",
    "prettier": "prettier --config .prettierrc --check --write \"./src/**/*.{tsx,jsx,ts,js}\"",
    "check-types": "tsc --noemit -p tsconfig.json",
    "transifex:push-server-strings": "TOKEN=$(npx dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_TOKEN) SECRET=$(npx dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_SECRET) node ./server-side-translations/push-server-strings.js",
    "transifex:push": "npm run transifex:push-server-strings && npx txjs-cli push src/ --token=$(npx dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_TOKEN) --secret=$(npx dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_SECRET)",
    "transifex:purge": "npx txjs-cli push src/ --token=$(npx dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_TOKEN) --secret=$(npx dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_SECRET) --purge",
    "transifex:refresh": "npx txjs-cli invalidate --token=$(npx dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_TOKEN) --secret=$(npx dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_SECRET)"
  },
