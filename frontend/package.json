{
  "name": "resilience-atlas-react",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "cypress run --headless --browser chrome",
    "test:open": "cypress open",
    "lint": "next lint",
    "prettier": "prettier --config .prettierrc.json --check --write \"./src/**/*.{tsx,jsx,ts,js}\"",
    "check-types": "tsc --noemit -p tsconfig.json",
    "transifex:push-server-strings": "TOKEN=$(./node_modules/.bin/dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_TOKEN) SECRET=$(./node_modules/.bin/dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_SECRET) node ./server-side-translations/push-server-strings.js",
    "transifex:push": "yarn run transifex:push-server-strings && txjs-cli push src/ --token=$(./node_modules/.bin/dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_TOKEN) --secret=$(./node_modules/.bin/dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_SECRET)",
    "transifex:purge": "txjs-cli push src/ --token=$(./node_modules/.bin/dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_TOKEN) --secret=$(./node_modules/.bin/dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_SECRET) --purge",
    "transifex:refresh": "txjs-cli invalidate --token=$(./node_modules/.bin/dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_TOKEN) --secret=$(./node_modules/.bin/dotenv -e .env -c production -p NEXT_PUBLIC_TRANSIFEX_SECRET)"
  },
  "dependencies": {
