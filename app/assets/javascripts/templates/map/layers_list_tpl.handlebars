<ul>
  {{#each groups}}
    {{!-- {{#if categories}} --}}
      <li class="group" data-slug="{{slug}}">
        <div class="m-layers-list-header">
          <div class="header-title">{{name}}</div>
        </div>
        <ul>
          {{#each layers}}
            <li class="layer {{#ifcond opacity_text}}{{/ifcond}}  {{#if dashboard_order}} {{dashboard_order}} {{/if}}">
              <div class="panel-item-switch m-form-input--switch">
                <input type="checkbox" class="panel-input-switch" id="layer_{{id}}" {{#if active}}checked{{/if}}>
                <label for="layer_{{id}}"></label>
              </div>
              <div class="panel-item-title">{{name}}</div>
              <button class="panel-trasparecy-switcher">
                <svg class="icon-settings" opacityLevel="{{opacity_text}}"width="9px" height="14px" viewBox="0 0 9 14" version="1.1">
                  <g id="Map" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd" sketch:type="MSPage">
                    <g id="Map_UI-amendments" sketch:type="MSArtboardGroup" transform="translate(-321.000000, -325.000000)" stroke="#C0C0C0" fill="#FFFFFF">
                      <path d="M325.5,339 C327.985281,339 330,336.985281 330,334.5 C330,332.014719 327.428571,330.090909 325.5,325 C323.571429,330.090909 321,332.014719 321,334.5 C321,336.985281 323.014719,339 325.5,339 Z" id="opacity" sketch:type="MSShapeGroup"></path>
                    </g>
                  </g>
                </svg>
              </button>
              {{#if info}}
                <button class="btn-info" data-info="{{info}}" data-name="{{name}}">
                  <svg class="icon icon-info">
                    <use xlink:href="#icon-info"></use>
                  </svg>
                </button>
              {{/if}}
              {{#if download}}<a href="{{download_url}}" class="btn-download" attr="download">
                <svg class="icon icon-downloads" opacityLevel="{{opacity_text}}">
                  <use xlink:href="#icon-downloads"></use>
                </svg>
              </a>{{/if}}
              <div class="panel-item-slider">
                <div class="m-form-input--slider">
                  <div class="slider-wrapper">
                    <input data-id="{{id}}" type=range min=0 max=100 value={{opacity_text}} id=fader step=1 class="opacity-range">
                    <span class="opacity" style="width:{{opacity_text}}%;"></span>
                  </div>
                  <input type="number" class="opacity-teller" layer="{{id}}" value={{opacity_text}}><span class="unit">%</span>
                </div>
              </div>
            </li>
          {{/each}}
          {{#each categories}}
              <li class="category">
                <div class="m-layers-list-header {{#if active}}is-active{{/if}}">
                  <div class="header-title">{{name}}</div>
                  <div id="categoryHeader_{{id}}" class="header-switch m-form-input--switch"></div>
                </div>
                <ul class="m-layers-list-panel {{#if active}}is-active{{/if}}">
                  {{#each layers}}
                    <li class="layer {{#ifcond opacity_text}}{{/ifcond}}">
                      <div class="panel-item-switch m-form-input--switch">
                        <input type="checkbox" class="panel-input-switch" id="layer_{{id}}" {{#if active}}checked{{/if}}>
                        <label for="layer_{{id}}"></label>
                      </div>
                      <div class="panel-item-title">{{name}}</div>
                      <button class="panel-trasparecy-switcher">
                        <svg class="icon-settings" opacityLevel="{{opacity_text}}"width="9px" height="14px" viewBox="0 0 9 14" version="1.1">
                          <g id="Map" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd" sketch:type="MSPage">
                            <g id="Map_UI-amendments" sketch:type="MSArtboardGroup" transform="translate(-321.000000, -325.000000)" stroke="#C0C0C0" fill="#FFFFFF">
                              <path d="M325.5,339 C327.985281,339 330,336.985281 330,334.5 C330,332.014719 327.428571,330.090909 325.5,325 C323.571429,330.090909 321,332.014719 321,334.5 C321,336.985281 323.014719,339 325.5,339 Z" id="opacity" sketch:type="MSShapeGroup"></path>
                            </g>
                          </g>
                        </svg>
                      </button>
                      {{#if info}}
                        <button class="btn-info" data-info="{{info}}" data-name="{{name}}">
                          <svg class="icon icon-info">
                            <use xlink:href="#icon-info"></use>
                          </svg>
                        </button>
                      {{/if}}
                      {{#if download}}<a href="{{download_url}}" class="btn-download" attr="download">
                        <svg class="icon icon-downloads" opacityLevel="{{opacity_text}}">
                          <use xlink:href="#icon-downloads"></use>
                        </svg>
                      </a>{{/if}}
                      <div class="panel-item-slider">
                        <div class="m-form-input--slider">
                          <div class="slider-wrapper">
                            <input data-id="{{id}}" type=range min=0 max=100 value={{opacity_text}} id=fader step=1 class="opacity-range">
                            <span class="opacity" style="width:{{opacity_text}}%;"></span>
                          </div>
                          <input type="number" class="opacity-teller" layer="{{id}}" value={{opacity_text}}><span class="unit">%</span>
                        </div>
                      </div>
                    </li>
                  {{/each}}

                  {{#each subcategory}}
